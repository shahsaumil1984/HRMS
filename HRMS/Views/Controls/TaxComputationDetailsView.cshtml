<div class="middlewrap">
    <div class="row detailsWindow" id="taxComputationDetailsWindow">
        <form id="taxComputationDetailsForm"
              name="taxComputationDetailsForm"
              data-ui-binding="true"
              data-service-object="taxComputationService"
              data-associated-list="taxComputationGrid"
              method="get" class="col-xs-12 col-sm-12" action="" role="form">

            <div ng-show="model.TaxComputationID != 0">
                <h3>Edit Tax Computation</h3>
            </div>
            <h3 ng-show="model.TaxComputationID == 0">Create New Tax Computation</h3>
            <div id="taxComputationrrorDisplay" class="taxComputationrrorDisplay" data-error-display="true"></div>

            <div class="row form-horizontal">
                <div class="col-md-5 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label class="col-md-4 control-label text-left" for="em_Year">Financial Year <i class="lblRequired">*</i> :</label>
                        <div class="col-md-4">
                            <select id="em_Year" name="em_Year" class="required form-control auto-width" ng-model="model.Year" value="{{model.Year}}">
                                <option value="">Select Financial Year</option>
                                @foreach (var year in Model.Years)
                                {
                                    <option value="@year">@year - @(year + 1)</option>
                                }
                            </select>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label class="col-md-5 control-label text-right" for="em_EPF">EPF:</label>
                        <div class="col-md-7">
                            <input type="text" name="em_EPF" id="em_EPF" ng-model="model.EPF" class="form-control number auto-width" maxlength="4" />
                        </div>
                    </div>
                </div>

            </div>


            <p class="midtitle">Employee Details</p>
            <div class="row">
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label>Employee Code:</label>
                        <label>@ViewBag.EmployeeCode</label>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="form-group">
                        <label>Employee Name:</label>
